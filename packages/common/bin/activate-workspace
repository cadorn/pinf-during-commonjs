#!/bin/bash

###################################################################################
# NOTE: This file is generated by PINF. Do NOT modify. Your changes may get lost! #
###################################################################################

function enquote_all () {
    ARGS=""
    for ARG in "$@"; do
        [ -n "$ARGS" ] && ARGS="$ARGS "
        ARGS="$ARGS'""$(echo " $ARG" | cut -c 2- | sed 's/'"'"'/'"'"'"'"'"'"'"'"'/g')""'"
    done
    echo "$ARGS"
}

export PINF_WORKSPACE_HOME=__PINF_WORKSPACE_HOME__
export PINF_PLATFORM_HOME=__PINF_PLATFORM_HOME__

if [ "$PINF_OLD_PATH" != "" ]; then
  export PATH="$PINF_OLD_PATH"
fi
export PINF_OLD_PATH="$PATH"

source "$PINF_PLATFORM_HOME/bin/activate.bash"

if [ "$#" -lt 1 ]; then
    __SHELL_CUSTOMIZATIONS__
    cd "$PINF_WORKSPACE_HOME"
    "$SHELL"
else
    "$SHELL" -c "$(enquote_all "$@")"
fi
